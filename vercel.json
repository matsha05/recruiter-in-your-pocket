{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.json",
            "use": "@vercel/static"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "assets/**",
            "use": "@vercel/static"
        },
        {
            "src": "styles/**",
            "use": "@vercel/static"
        },
        {
            "src": "scripts/**",
            "use": "@vercel/static"
        },
        {
            "src": "research/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/sample-report.json",
            "dest": "/sample-report.json"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/styles/(.*)",
            "dest": "/styles/$1"
        },
        {
            "src": "/scripts/(.*)",
            "dest": "/scripts/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/(health|ready)",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)\\.html",
            "dest": "/$1.html"
        },
        {
            "src": "/(workspace|terms|privacy|research)",
            "dest": "/server.js"
        },
        {
            "src": "/research/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}